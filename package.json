{
  "name": "pptx-translator-pro",
  "version": "1.0.0",
  "description": "Professional PowerPoint Translation Tool with Google Drive Integration",
  "type": "module",
  "browser": {
    "googleapis": false,
    "google-auth-library": false,
    "jszip": false,
    "xml2js": false,
    "pptxgenjs": false,
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup": "chmod +x setup.sh && ./setup.sh",
    "fix-git": "chmod +x fix-git.sh && ./fix-git.sh",
    "fix-netlify": "chmod +x fix-netlify.sh && ./fix-netlify.sh",
    "fix-postcss": "chmod +x fix-postcss.sh && ./fix-postcss.sh",
    "fix-tailwind": "chmod +x fix-tailwind.sh && ./fix-tailwind.sh",
    "fix-dependencies": "chmod +x fix-dependencies.sh && ./fix-dependencies.sh",
    "fix-all-imports": "chmod +x fix-all-imports.sh && ./fix-all-imports.sh",
    "fix-final": "chmod +x fix-final.sh && ./fix-final.sh",
    "fix-browser": "chmod +x fix-browser.sh && ./fix-browser.sh",
    "fix-versioned-imports": "chmod +x fix-versioned-imports.sh && ./fix-versioned-imports.sh",
    "deploy": "npm run build && netlify deploy --prod --dir=dist",
    "test": "echo 'No tests configured' && exit 0"
  },
  "keywords": [
    "powerpoint",
    "translation",
    "pptx",
    "google-translate",
    "react",
    "typescript"
  ],
  "author": "Bartosz Idzik Enterprise Ecosystem",
  "license": "MIT",
